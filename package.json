{
  "name": "temporal-hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "lint": "eslint .",
    "start": "ts-node src/worker.ts",
    "start.watch": "nodemon src/worker.ts",
    "start.server": "ts-node server/app.ts",
    "start.all" : "nf start",
    "workflow": "ts-node src/client.ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "mocha --exit --require ts-node/register --require source-map-support/register src/mocha/*.test.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@atproto/api": "^0.6.23",
    "@temporalio/activity": "^1.8.4",
    "@temporalio/client": "^1.8.4",
    "@temporalio/worker": "^1.8.4",
    "@temporalio/workflow": "^1.8.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "masto": "^6.3.3",
    "nanoid": "3.x",
    "next": "^14.0.0",
    "twitter-api-v2": "^1.15.2"
  },
  "devDependencies": {
    "@temporalio/testing": "^1.8.4",
    "@tsconfig/node18": "^1.0.0",
    "@types/express": "^4.17.21",
    "@types/mocha": "8.x",
    "@types/node": "^16.11.43",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "foreman": "^3.0.1",
    "mocha": "8.x",
    "nodemon": "^2.0.12",
    "prettier": "^2.8.8",
    "ts-node": "^10.8.1",
    "typescript": "^4.4.2"
  }
}
